<!-- boolean logic -->

<gxl>
 <graph ID='boolean' edgemode='directed'>
   <!-- logic values -->
   <node ID='T'/>
   <node ID='F'/>

   <!-- logic order -->
   <edge from='T' to='F'>
    <type value='above'/>
   </edge>
 
   <!-- negation order -->
   <edge from='T' to='F'>
     <type value='neg'/>
   </edge>

   <edge from='F' to='T'>
     <type value='neg'/>
   </edge>

 </graph>
</gxl>