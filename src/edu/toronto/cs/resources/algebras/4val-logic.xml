<!-- 4-val TT TF FT FF -->

<gxl xmlns:xbel='www.cs.toronto.edu/xbel'
     xmlns:xlink='xlink'>
 <graph ID='4-val' edgemode='directed'>
   <xbel:img xlink:type='simple' xlink:href='play/gxl/4val.gif'/>
   <!-- logic values -->
   <node ID='TT'/>
   <node ID='TF'/>
   <node ID='FT'/>
   <node ID='FF'/>

   <!-- logic order -->
   <edge from='TT' to='TF'>
    <type value='above'/>
   </edge>

   <edge from='TT' to='FT'>
    <type value='above'/>
   </edge>
 
   <edge from='TF' to='FF'>
    <type value='above'/>
   </edge>
   
   <edge from='FT' to='FF'>
    <type value='above'/>
   </edge>
   
   <!-- negation order -->
   <edge from='TT' to='FF'>
     <type value='neg'/>
   </edge>

   <edge from='FF' to='TT'>
     <type value='neg'/>
   </edge>
   
   <edge from='FT' to='TF'>
     <type value='neg'/>
   </edge>
   
   <edge from='TF' to='FT'>
     <type value='neg'/>
   </edge>

 </graph>
</gxl>
